CREATE TABLE customer (
    id INT AUTO_INCREMENT NOT NULL,
    first_name VARCHAR(50),
    last_name VARCHAR(50),
    age INT,
    PRIMARY KEY(id)
    );

CREATE TABLE orders (
    id INT AUTO_INCREMENT NOT NULL,
    units INT NOT NULL,
    customer_id INT,
    PRIMARY KEY(id),
    FOREIGN KEY (customer_id) REFERENCES customer(id)
    );